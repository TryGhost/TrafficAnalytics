{
  "name": "traffic-analytics",
  "version": "0.0.0",
  "repository": "git@github.com:TryGhost/TrafficAnalytics.git",
  "author": "Ghost Foundation",
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "start": "node dist/server.js",
    "pretest": "yarn build",
    "test:unit": "NODE_ENV=testing vitest run --coverage",
    "test": "yarn test:unit",
    "lint:code": "eslint *.js *.ts src/ test/ --ext .js,.ts --cache",
    "lint:code:fix": "eslint *.js *.ts src/ test/ --ext .js,.ts --cache --fix",
    "lint:code:ci": "eslint *.js *.ts src/ test/ --ext .js,.ts --cache --max-warnings 9999 || true",
    "lint": "yarn lint:code:ci",
    "preship": "yarn test",
    "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then yarn version && git push --follow-tags; fi"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/node": "^22.14.1",
    "@types/ua-parser-js": "^0.7.39",
    "c8": "^10.1.3",
    "eslint": "8.57.1",
    "eslint-plugin-ghost": "3.4.0",
    "mocha": "11.1.0",
    "sinon": "^20.0.0",
    "supertest": "6.3.4",
    "typescript": "^5.2.2",
    "vite": "^6.3.1",
    "vite-plugin-node": "^5.0.1",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@fastify/cors": "11.0.1",
    "@fastify/http-proxy": "11.1.2",
    "@tryghost/errors": "^1.3.7",
    "@vitest/coverage-v8": "^3.1.1",
    "dotenv": "16.5.0",
    "fastify": "5.3.0",
    "pino-pretty": "10.3.1",
    "ua-parser-js": "1.0.40"
  }
}
