{
  "name": "traffic-analytics",
  "version": "0.0.0",
  "repository": "git@github.com:TryGhost/TrafficAnalytics.git",
  "author": "Ghost Foundation",
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --watch src --watch server.js server.js",
    "start": "node server.js",
    "test:unit": "NODE_ENV=testing c8 --all --include 'src/**' --reporter text --reporter cobertura mocha './test/**/*.test.js'",
    "test": "yarn test:unit",
    "lint:code": "eslint *.js src/ test/ --ext .js --cache",
    "lint": "yarn lint:code",
    "preship": "yarn test",
    "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then yarn version && git push --follow-tags; fi"
  },
  "files": [
    "server.js",
    "src"
  ],
  "devDependencies": {
    "c8": "10.1.3",
    "eslint": "8.57.1",
    "eslint-plugin-ghost": "3.4.0",
    "mocha": "11.1.0",
    "nodemon": "3.1.9",
    "sinon": "20.0.0",
    "supertest": "6.3.4"
  },
  "dependencies": {
    "@fastify/cors": "11.0.1",
    "@fastify/http-proxy": "11.1.2",
    "@tryghost/errors": "^1.3.7",
    "dotenv": "16.5.0",
    "fastify": "5.3.0",
    "pino-pretty": "10.3.1",
    "ua-parser-js": "1.0.40"
  }
}
